/	"Calculez une factorielle d'un nombre
	fact(nbr=nbr) -> nbr;
"fact:
:
	1
	>if1
		>set> _multiple = +0
		>set> _resulta = +1
		>get> _calcul_fac(nbr, _multiple, _resulta)
		>del> _multiple
		>del> _resulta
	>if0
		>get> O
:;
"_multiplication: =#_resulta#*#_multiple#;
"_calcul_fac:
:
nbr > _multiple
>if1
	>add> _resulta + _multiplication(_resulta, _multiple)
	>add> _multiple + +1
	>get> _calcul_fac(_resulta, _multiple, nbr)
>if0
	>get> _resulta
:
