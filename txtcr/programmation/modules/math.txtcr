/	"Calculez la suite de Fibonacci d'un nombre
	fibo(nbr1=nbr,nbr2=nbr,limite=nbr) -> nbr
	Par dÃ©faut : nbr1 = +0, nbr2 = +1;
"fibo:
:
1
>if1
	>set> _boucle = +0
	>set> _nbr_prcdt = nbr1
	>set> _nbr_a_add = nbr2
	>get> _calcul_fibo(_nbr_prcdt, _nbr_a_add, _boucle, limite)
	>del> nbr1
	>del> nbr2
	>del> _nbr_prcdt
	>del> _nbr_a_add
	>del> __nbr_a_add
:;
"_calcul_fibo:
:
#_boucle# < #limite#
>if1
	>add> _boucle + +1
	>set> __nbr_a_add = _nbr_prcdt
	>add> _nbr_prcdt + _nbr_a_add
	>set> _nbr_a_add = __nbr_a_add
	>get> _calcul_fibo(_nbr_prcdt, _nbr_a_add, _boucle, limite)
>if0
	>get> _nbr_prcdt
:
